<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
	<managed-bean>
		<managed-bean-name>registerBean</managed-bean-name>
		<managed-bean-class>com.neoalgoritma.controller.pub.RegisterBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>subscriptionBean</managed-bean-name>
		<managed-bean-class>com.neoalgoritma.controller.secured.SubscriptionBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>securedIndexBean</managed-bean-name>
		<managed-bean-class>com.neoalgoritma.controller.secured.SecuredIndexBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>com.neoalgoritma.controller.pub.LoginBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>posStatusBean</managed-bean-name>
		<managed-bean-class>com.neoalgoritma.controller.secured.PosStatusBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>posNewCustomerBean</managed-bean-name>
		<managed-bean-class>com.neoalgoritma.pos.controller.customer.PosNewCustomerBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>customersBean</managed-bean-name>
		<managed-bean-class>com.neoalgoritma.pos.controller.customer.CustomersBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

</faces-config>
